{
    "name": "juanrube/laravel-ui-avatars",
    "description": "Fork of Rackbeat/laravel-ui-avatars. Official Laravel wrapper around ui-avatars.com and LasseRafn/php-initial-avatar-generator",
    "keywords": [
        "laravel",
        "ui-avatars",
        "php",
        "avatars",
        "initials"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lasse Rafn",
            "email": "lasserafn@gmail.com"
        },
        {
            "name": "Rackbeat",
            "email": "open-source@rackbeat.com"
        },
        {
            "name": "juanrube",
            "email": "jmrube10@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "lasserafn/php-initial-avatar-generator": "^4.4",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.7",
        "phpunit/phpunit": "^7.0|^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Rackbeat\\UIAvatars\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UIAvatars\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rackbeat\\UIAvatars\\UIAvatarsServiceProvider"
            ]
        }
    }
}
